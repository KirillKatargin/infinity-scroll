{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "includePattern": "\\.(vue|js)$"
  },
  "plugins": ["plugins/markdown", "node_modules/jsdoc-vuejs"],
  "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
  },
  "opts": {
    "destination": "docs",
    "recurse": true,
    "readme": "README.md"
  }
}
